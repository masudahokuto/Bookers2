<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<p><%= link_to "show画面遷移", user_path(current_user.id) %></p>
<%= form_with model: @book do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Opinion</h4>
  <%= f.text_area :opinion %>
  <%= f.submit 'Create Book' %>
<% end %>

<% @users.each do |user| %>
  <br>
  <br>
  <%= user.name %>
  <br>
  <br>
<% end %>

